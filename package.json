{
  "name": "api-sascha",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "8.9.1"
  },
  "scripts": {
    "start": "node ./app.js",
    "start:dev": "nodemon ./app.js",
    "update:db": "psql saschadb < $PWD/db/saschadb.sql",
    "remove:db": "dropdb -e --if-exists -h localhost -p 5432 -U postgres saschadb",
    "create:db": "sudo -u postgres psql -c 'create database saschadb;'",
    "histori:db": "pg_dump saschadb -h localhost -p 5432 -U postgres -F p -b -v -f $PWD/db/saschadb$(date +%s).hsql",
    "backup:db": "npm run histori:db && pg_dump saschadb -h localhost -p 5432 -U postgres -F p -b -f $PWD/db/saschadb.sql",
    "restore:db": "npm run remove:db && npm run create:db && npm run update:db",
    "backup:db:plain": "heroku run 'pg_dump $DATABASE_URL' > saschadb_heroku.sql",
    "sascha": "bash sascha"
  },
  "dependencies": {
    "bcryptjs": "^1.1.0",
    "bluebird": "^3.5.1",
    "body-parser": "~1.17.1",
    "bookshelf": "^0.12.1",
    "cloudinary": "^1.9.1",
    "debug": "~2.6.3",
    "express": "^4.15.5",
    "express-handlebars": "^3.0.0",
    "graceful-fs": "^4.1.11",
    "jwt-simple": "^0.5.1",
    "knex": "^0.13.0",
    "lodash": "^4.17.5",
    "moment": "^2.21.0",
    "morgan": "^1.9.0",
    "multer": "^0.1.8",
    "nodemailer": "^4.6.3",
    "pg": "^7.4.1",
    "socket.io": "^2.1.0",
    "when": "^3.7.8"
  },
  "devDependencies": {
    "nodemon": "^1.17.3"
  }
}
